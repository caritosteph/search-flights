{
  "name": "nodetest",
  "version": "1.0.0",
  "description": "Locomote - Node Test",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src open:back lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:back": "babel-node tools/backServer.js",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "open:dist": "babel-node tools/distServer.js",
    "build": "npm run clean-dist && babel-node tools/build.js && npm run open:dist",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caritosteph/node.locomote.test.git"
  },
  "author": "Carolyn Ulfe",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "^6.18.0",
    "browser-sync": "2.18.5",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "npm-run-all": "3.1.2",
    "request": "^2.79.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  }
}
